		<property name="lat3" type="double" column="LAT3"/>
		<property name="long4" type="double" column="LONG4"/>
		<property name="lat4" type="double" column="LAT4"/>
		<property name="long5" type="double" column="LONG5"/>
		<property name="lat5" type="double" column="LAT5"/>
		<property name="long6" type="double" column="LONG6"/>
		<property name="lat6" type="double" column="LAT6"/>
		<property name="long7" type="double" column="LONG7"/>
		<property name="lat7" type="double" column="LAT7"/>
		<property name="long8" type="double" column="LONG8"/>
		<property name="lat8" type="double" column="LAT8"/>
		<property name="long9" type="double" column="LONG9"/>
		<property name="lat9" type="double" column="LAT9"/>
		<property name="long10" type="double" column="LONG10"/>
		<property name="lat10" type="double" column="LAT10"/>
	</class>
</hibernate-mapping>