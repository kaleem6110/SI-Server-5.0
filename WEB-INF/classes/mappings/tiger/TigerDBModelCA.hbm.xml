<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.spacetimeinsight.db.geocoder.model">

	<class name="TigerDBModelCA" table="TIGER_CA">
		<id name="id" type="long">
            <generator class="native" />
        </id>   
		<property name="tlid" type="long" column="TLID" index="IDX0_TIGER_CA"/>
        <property name="feDirp" type="string" column="FEDIRP" length="2"/>
		<property name="feName" type="string" column="FENAME" length="30" index="IDX1_TIGER_CA"/>
