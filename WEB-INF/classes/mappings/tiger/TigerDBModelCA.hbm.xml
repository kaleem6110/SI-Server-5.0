		<property name="toAddr" type="string" column="TOADDR" length="11" index="IDX5_TIGER_CA"/>
		<property name="zipl" type="string" column="ZIPL" length="5" index="IDX6_TIGER_CA"/>
		<property name="zipr" type="string" column="ZIPR" length="5" index="IDX7_TIGER_CA"/>
		<property name="frLong" type="double" column="FRLONG"/>
		<property name="frLat" type="double" column="FRLAT"/>
		<property name="toLong" type="double" column="TOLONG"/>
		<property name="toLat" type="double" column="TOLAT"/>
		<property name="rtsq" type="double" column="RTSQ"/>
		<property name="long1" type="double" column="LONG1"/>
		<property name="lat1" type="double" column="LAT1"/>
		<property name="long2" type="double" column="LONG2"/>
		<property name="lat2" type="double" column="LAT2"/>
