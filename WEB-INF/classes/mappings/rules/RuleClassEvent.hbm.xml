<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping
	package="com.spacetimeinsight.db.rules.model">
	<class name="RuleClassEvent" table="STI_R_EVENT_CLASS">
		<id name="id" type="long">
			<generator class="native" />
		</id>
		<property name="ruleEventId" type="long" column="RULE_EVENT_ID" not-null="true"/>
		<property name="className" type="string" column="CLASSNAME" not-null="true"/>
		<property name="createdDate" type="timestamp" column="CREATED_DATE"/>
        <property name="modifiedDate" type="timestamp" column="MODIFIED_DATE"/>
        <property name="createdBy" type="string" column="CREATED_BY"/>
		<property name="modifiedBy" type="string" column="MODIFIED_BY"/>
	</class>
</hibernate-mapping>