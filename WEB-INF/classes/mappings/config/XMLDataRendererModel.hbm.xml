<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping
    package="com.spacetimeinsight.db.config.model">
	
    <class name="XMLDataRendererModel" table="STI_C_XML_DATA_RENDERERS">
        <id name="id" type="long">
            <generator class="native" />
        </id>   
        <property name="name" type="string" column="NAME" not-null="true" unique="true"/>
        <property name="type" type="string" column="TYPE" not-null="true" unique="true"/>
        <property name="domainId" type="long" column="DOMAIN_ID" not-null="true"/>
        <property name="createdDate" type="timestamp" column="CREATED_DATE"/>
        <property name="modifiedDate" type="timestamp" column="MODIFIED_DATE"/>
        <property name="createdBy" type="string" column="CREATED_BY"/>
		<property name="modifiedBy" type="string" column="MODIFIED_BY"/>
    </class>
</hibernate-mapping>